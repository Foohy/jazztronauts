"VertexlitGeneric"
{
	"$basetexture" "models/weapons/v_p90/v_p90"
	"$bumpmap" 		"models/weapons/v_p90/v_p90_normal"
 	 "$envmap" "env_cubemap"
	 "$envmaptint" "[.02 .02 .02]"
   	 "$envmapsaturation" .5
  	  "$model" 1

   	 "$phong" "1"
   	 "$phongexponent" "10"
   	 "$phongboost"    ".7"
   	 "$phongfresnelranges"    "[2 4 1]"
	
	// Blood effects
	$detail "models/blood.vtf"
	$detailscale "1.8"
	$detailblendfactor .001
	$detailblendmode 0
	$detailAmount 0
	Proxies
	{
		WeaponBlood
		{
			resultVar $detailAmount
		}
		Equals
		{
			srcVar1 $detailAmount
			resultVar $detailblendfactor			
		}
	}
}
